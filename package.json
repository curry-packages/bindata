{
    "name": "bindata",
    "version": "3.0.0",
    "author": "Michael Hanus <mh@informatik.uni-kiel.de>",
    "synopsis": "Library to support reading/writing binary data",
    "category": [ "Data" ],
    "dependencies": {
        "base"   : ">= 3.0.0, < 4.0.0",
        "process": ">= 3.0.0, < 4.0.0"
    },
    "exportedModules": [ "System.IO.BinaryFile" ],
    "compilerCompatibility": {
        "pakcs"   : ">= 3.0.0, < 4.0.0",
        "kics2"   : ">= 3.0.0, < 4.0.0",
        "curry2go": ">= 1.0.0",
        "kmcc"    : ">= 0.1.0"
    },
    "testsuite": {
        "src-dir": "test",
        "modules": [ "BasicTest" ]
    },
    "license": "BSD-3-Clause",
    "licenseFile": "LICENSE",
    "source": {
        "git": "https://git.ps.informatik.uni-kiel.de/curry-packages/bindata.git",
        "tag": "$version"
    }
}
